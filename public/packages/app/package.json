    "axios": "^0.24.0",
    "crypto-js": "^4.1.1",
    "dayjs": "1.11.6",
    "echarts": "^5.4.0",
    "echarts-for-react": "^3.0.2",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "^8.0.3",
    "react-redux": "^8.0.2",
